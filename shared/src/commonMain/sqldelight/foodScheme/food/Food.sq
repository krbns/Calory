CREATE TABLE Food(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    calories REAL NOT NULL,
    proteins REAL NOT NULL,
    fats REAL NOT NULL,
    carbs REAL NOT NULL
);

count:
SELECT COUNT(*) FROM Food;

insertFood:
INSERT INTO Food(name, calories, proteins, fats, carbs)
VALUES (?, ?, ?, ?, ?);

findByName:
SELECT * FROM Food WHERE LOWER(name) = LOWER(?);

search:
SELECT * FROM Food WHERE LOWER(name) LIKE LOWER(?);